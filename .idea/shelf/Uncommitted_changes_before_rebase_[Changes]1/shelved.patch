+                      if (_phoneController!.text.length < 10) {
+                        showToast_Failure_Message(
+                          context,
+                          "Mobile number should be 10 digits",
+                        );
+                      } else {
+                        LoaderDialogView(context).showLoadingDialog();
+                        await authProvider.mobileNumberVerification(
