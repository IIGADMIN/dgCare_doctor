+                      },
+                      child: Text(
+                        "Get OTP",
+                        style: boldTextStyle(color: Colors.white),
+                      )).paddingSymmetric(horizontal: 10),
+                ],
+              ),
+            ),
+          );
+        } else {
+          return noInternetScreen(context);
+        }
+      },
